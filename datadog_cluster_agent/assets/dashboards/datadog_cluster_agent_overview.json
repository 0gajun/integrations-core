{
    "author_info": {
        "author_name": "Datadog"
    },
    "board_title": "Datadog Cluster Agent Overview",
    "description": "Our Datadog Cluster Agent dashboard gives you an overview of the cluster checks, external metrics and admission controller stats in your clusters. For more information about the Cluster Agent, check out [Cluster Agent for Kubernetes](https://docs.datadoghq.com/agent/cluster_agent/).",
    "template_variables": [
        {
            "default": "*",
            "name": "cluster",
            "prefix": "cluster_name"
        }
    ],
    "widgets": [
        {
            "height": 13,
            "tile_def": {
                "autoscale": true,
                "custom_links": [],
                "precision": 2,
                "requests": [
                    {
                        "aggregator": "avg",
                        "q": "sum:datadog.cluster_agent.external_metrics{$cluster,is_leader:true}"
                    }
                ],
                "viz": "query_value"
            },
            "title": true,
            "title_align": "left",
            "title_size": 16,
            "title_text": "External Metrics",
            "type": "query_value",
            "width": 22,
            "x": 1,
            "y": 47
        },
        {
            "height": 13,
            "tile_def": {
                "autoscale": true,
                "custom_links": [],
                "precision": 2,
                "requests": [
                    {
                        "aggregator": "last",
                        "q": "sum:datadog.cluster_agent.cluster_checks_nodes_reporting{$cluster,is_leader:true}"
                    }
                ],
                "viz": "query_value"
            },
            "title": true,
            "title_align": "left",
            "title_size": 16,
            "title_text": "Workers",

instances:
<% @instances.each do |i| -%>
  - monitor_agent_url: <%= i['monitor_agent_url'] %>
  <% if i.key?('plugin_ids') -%>
    plugin_ids:
    <% i['plugin_ids'].each do |p| -%>
    - <%= p %>
    <% end -%>
  <% end -%>
  <% if i.key?('tags') -%>
    tags:
    <% i['tags'].each do |t| -%>
    - <%= t %>
    <% end -%>
  <% end -%>
<% end -%>

# Nothing to configure here
init_config:
